@model Cieros.Models.RankClass

@{
    ViewBag.Title = "Rank Class Details";
}
<div class="container-fluid">
    <h2 class="h3 mb-2 text-gray-800">Rank Class Details </h2>


    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary"> Rank Class Details  |     @Html.ActionLink("Back to List", "Index")</h6>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <div class="col-lg-6 ">
                 
                    <table class="table table-bordered table-striped">

                        <tr>
                            <td> @Html.DisplayName("Class")</td>
                            <td>  @Html.DisplayFor(model => model.StdClass)</td>
                        </tr>
                        <tr>
                            <td>  @Html.DisplayName("Next Class")</td>
                            <td>  @Html.DisplayFor(model => model.NextClass)</td>
                        </tr>


                    </table>


                    <p>
                        @Html.ActionLink("Edit", "Edit", new { id = Model.StdClass }) |
                        @Html.ActionLink("Back to List", "Index")
                    </p>
                </div>
            </div>
        </div>
    </div>  
</div>